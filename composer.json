{
    "name": "ratno/nova-notification-feed",
    "description": "A Laravel Nova package that adds a notification feed in your Nova app.",
    "keywords": [
        "laravel",
        "nova",
        "broadcast",
        "notifications",
        "sidebar",
        "bell",
        "notice board"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "ratno/nova-echo": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ratno\\NovaNotificationFeed\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ratno\\NovaNotificationFeed\\NovaNotificationFeedServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
